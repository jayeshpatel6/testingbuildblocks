{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "actionName": {
            "type": "string",
            "defaultValue": "Email JayeshBAE Team_-EmailAction-"
        },
        "emailAddress": {
            "type": "string",
            "defaultValue": "jayeshbae@hotmail.com"
        }
    },
    "resources": [
        {
            "type": "Microsoft.Insights/actionGroups",
            "apiVersion": "2018-03-01",
            "name": "VYP Email Action Group",
            "location": "Global",
            "properties": {
                "groupShortName": "EmailJBAE",
                "enabled": true,
                "smsReceivers": [
                ],
                "emailReceivers": [
                    {
                        "name": "[parameters('actionName')]",
                        "emailAddress": "[parameters('emailAddress')]",
                        "status": "Enabled",
                        "useCommonAlertSchema": false
                    }
                ],
                "webhookReceivers": [
                ]
            }
        }
    ],
    "outputs": {
    }
}