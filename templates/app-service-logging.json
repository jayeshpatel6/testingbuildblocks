{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
  },
  "variables": {
  },
  "resources": [
    {
      "name": "logs",
      "type": "Microsoft.Web/sites/config",
      "location": "West Europe",
      "properties": {
        "applicationLogs": {
          "fileSystem": {
            "level": "Off"
          },
          "azureTableStorage": {
            "level": "Off",
            "sasUrl": null
          },
          "azureBlobStorage": {
            "level": "Verbose",
            "sasUrl": null,
            "retentionInDays": 99
          }
        },
        "httpLogs": {
          "fileSystem": {
            "retentionInMb": 99,
            "retentionInDays": 99,
            "enabled": true
          },
          "azureBlobStorage": {
            "sasUrl": null,
            "retentionInDays": 99,
            "enabled": false
          }
        },
        "failedRequestsTracing": {
          "enabled": true
        },
        "detailedErrorMessages": {
          "enabled": true
        }
      }
    }
  ],
  "outputs": {

  }
}